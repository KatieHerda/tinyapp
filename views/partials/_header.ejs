<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <% if (user) { %>
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
        <% } %>
      </div>
    
    </div>
        <div>
          <% if (!user) { %>
            <button type="submit"; style="background-color: white; border: 1.5px solid blue; border-radius: 5px;color: blue"><a href="/login">Login</a></button>
            <button type="submit"; style="background-color: white; border: 1.5px solid blue; border-radius: 5px;color: blue"><a href="/register">Register</a></button>
            <% } else { %>
                <h5>Logged in as: <%= user.email %></h5>
                <form method="POST" action="/logout">
                <button type="submit"; style="background-color: white; border-color: rgb(75, 77, 182); color: rgb(75, 77, 182); border-radius: 5px">Logout</button>
            </form>
              <% } %>
        </div>
  </nav>
</header>
